{
  "testEnvironment": "jsdom",
  "setupFilesAfterEnv": [
    "<rootDir>/setupTests.ts"
  ],
  "collectCoverageFrom": [
    "<rootDir>/src/**/*.tsx",
    "<rootDir>/src/**/utils/*.ts",
    "!<rootDir>/src/main.tsx",
    "!<rootDir>/src/components/molecules/ModalPortal/**",
    "!<rootDir>/src/components/templates/**",
    "!<rootDir>/src/routes/**",
    "!<rootDir>/src/pages/**",
    "!<rootDir>/src/hooks/**"
  ],
  "moduleNameMapper": {
    "\\.(css|less)$": "identity-obj-proxy",
    "\\.png": "<rootDir>/jest/__mocks__/fileMock.js",
    "\\.svg": "<rootDir>/jest/__mocks__/svgMock.js"
  }
}
